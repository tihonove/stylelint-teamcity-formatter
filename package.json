{
  "name": "stylelint-teamcity-formatter",
  "version": "2.0.0",
  "description": "Teamcity formatter for stylelint",
  "main": "index.cjs",
  "module": "index.js",
  "type": "module",
  "exports": {
    "require": "./index.cjs",
    "import": "./index.js"
  },
  "author": "tihonove <tihonov.ea@gmail.com>",
  "contributors": [
    {
      "name": "Eugene Tikhonov",
      "email": "tihonov.ea@gmail.com"
    }
  ],
  "scripts": {
  "build:cjs": "BABEL_ENV=cjs babel src/index.js --out-file index.cjs",
  "build:esm": "BABEL_ENV=esm babel src/index.js --out-file index.js",
    "build": "npm run build:cjs && npm run build:esm",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tihonove/stylelint-teamcity-formatter.git"
  },
  "bugs": {
    "url": "https://github.com/tihonove/stylelint-teamcity-formatter/issues"
  },
  "files": [
    "index.js",
    "index.cjs",
    "src/"
  ],
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3"
  },
  "engines": {
    "node": "*"
  },
  "keywords": [
    "lint",
    "css lint",
    "style lint",
    "scss lint",
    "less lint",
    "stylelint",
    "javascript"
  ]
}
